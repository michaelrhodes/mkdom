{
  "name": "mkdom",
  "version": "1.0.1",
  "description": "Convert HTML into DOM elements in the browser and on the server.",
  "main": "index.js",
  "browser": "browser.js",
  "dependencies": {
    "domino": "~1.0.17"
  },
  "devDependencies": {
    "tape": "~1.1.1"
  },
  "scripts": {
    "test": "tape test/server/*.js"
  },
  "testling": {
    "files": "test/browser/*.js",
    "browsers": {
      "ie": [6, 7, 8, 9, 10],
      "chrome": [20, 25, 29],
      "firefox": [4, 7, 19, 24],
      "safari": [5.1, 6],
      "opera": [10, 12, 15],
      "iphone": [6],
      "android": [4.2]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelrhodes/mkdom"
  },
  "keywords": [
    "html",
    "dom",
    "browser",
    "server"
  ],
  "author": "Michael Rhodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrhodes/mkdom/issues"
  }
}
